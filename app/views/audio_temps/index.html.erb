<p id="notice"><%= notice %></p>

<h1>Listing Audio Temps</h1>

<table>
  <thead>
    <tr>
      <th>Audioname</th>
      <th>Flocation</th>
      <th>Fsize</th>
      <th>Ftlocation</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @audio_temps.each do |audio_temp| %>
      <tr>
        <td><%= audio_temp.audioname %></td>
        <td><%= audio_temp.flocation %></td>
        <td><%= audio_temp.fsize %></td>
        <td><%= audio_temp.ftlocation %></td>
        <td><%= audio_temp.status %></td>
        <td><%= link_to 'Show', audio_temp %></td>
        <td><%= link_to 'Edit', edit_audio_temp_path(audio_temp) %></td>
        <td><%= link_to 'Destroy', audio_temp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Audio temp', new_audio_temp_path %>
