<p id="notice"><%= notice %></p>

<h1>Listing Csv File Temps</h1>

<table>
  <thead>
    <tr>
      <th>Filename</th>
      <th>Flocation</th>
      <th>Fsize</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @csv_file_temps.each do |csv_file_temp| %>
      <tr>
        <td><%= csv_file_temp.filename %></td>
        <td><%= csv_file_temp.flocation %></td>
        <td><%= csv_file_temp.fsize %></td>
        <td><%= csv_file_temp.status %></td>
        <td><%= link_to 'Show', csv_file_temp %></td>
        <td><%= link_to 'Edit', edit_csv_file_temp_path(csv_file_temp) %></td>
        <td><%= link_to 'Destroy', csv_file_temp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Csv file temp', new_csv_file_temp_path %>
