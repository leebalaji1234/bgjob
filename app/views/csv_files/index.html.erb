<p id="notice"><%= notice %></p>

<h1>Listing Csv Files</h1>

<table>
  <thead>
    <tr>
      <th>Filename</th>
      <th>Flocation</th>
      <th>Fsize</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @csv_files.each do |csv_file| %>
      <tr>
        <td><%= csv_file.filename %></td>
        <td><%= csv_file.flocation %></td>
        <td><%= csv_file.fsize %></td>
        <td><%= csv_file.status %></td>
        <td><%= link_to 'Show', csv_file %></td>
        <td><%= link_to 'Edit', edit_csv_file_path(csv_file) %></td>
        <td><%= link_to 'Destroy', csv_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Csv file', new_csv_file_path %>
