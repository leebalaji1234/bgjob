<p id="notice"><%= notice %></p>

<h1>Listing Records</h1>

<table>
  <thead>
    <tr>
      <th>Guid</th>
      <th>Userid</th>
      <th>Callerid</th>
      <th>Source</th>
      <th>Destination</th>
      <th>Templateid</th>
      <th>Dialedtime</th>
      <th>Answeredtime</th>
      <th>Endtime</th>
      <th>Dialstatus</th>
      <th>Reason</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @records.each do |record| %>
      <tr>
        <td><%= record.guid %></td>
        <td><%= record.userid %></td>
        <td><%= record.callerid %></td>
        <td><%= record.source %></td>
        <td><%= record.destination %></td>
        <td><%= record.templateid %></td>
        <td><%= record.dialedtime %></td>
        <td><%= record.answeredtime %></td>
        <td><%= record.endtime %></td>
        <td><%= record.dialstatus %></td>
        <td><%= record.reason %></td>
        <td><%= link_to 'Show', record %></td>
        <td><%= link_to 'Edit', edit_record_path(record) %></td>
        <td><%= link_to 'Destroy', record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Record', new_record_path %>
